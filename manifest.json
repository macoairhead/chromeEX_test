{
    "manifest_version": 2,
    "name": "test",
    "description": "test",
    "version": "1.0",
    "icons": {
        "16": "images/icon/icon16.png",
        "48": "images/icon/icon48.png",
        "128": "images/icon/icon128.png"
    },
    "background": {
        "scripts": ["js/eventpage.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon/icon19.png",
            "38": "images/icon/icon38.png"
        },
        "default_title": "test",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["js/scripts.js"]
    }]
}